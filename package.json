{
  "name": "@pyriter/dashboard-iam-role",
  "version": "1.1.0",
  "description": "Dashboard IAM ROLE",
  "main": "dist/main.js",
  "type": "module",
  "typings": "dist/main.d.ts",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pyriter/dashboardIamRole.git"
  },
  "author": "Phong Vuong",
  "license": "GPL",
  "private": false,
  "bugs": {
    "url": "https://github.com/pyriter/dashboardIamRole/issues"
  },
  "homepage": "https://github.com/pyriter/dashboardIamRole#readme",
  "scripts": {
    "test": "jest --silent --detectOpenHandles --coverage --runInBand",
    "test-dev": "jest --watch --silent --detectOpenHandles --coverage",
    "clean": "jest --clearCache && rimraf dist",
    "build": "rollup --config && npm run test",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "rimraf": "rimraf",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint && npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/aws-lambda": "latest",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "ascjs": "^6.0.3",
    "husky": ">=6",
    "jest": "^29.7.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.5.2",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "clearMocks": true,
    "resetMocks": true,
    "resetModules": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "html"
    ],
    "testTimeout": 300000,
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "aws-cdk-lib": "^2.183.0",
    "constructs": "^10.4.2"
  }
}
