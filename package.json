{
  "name": "@pyriter/dashboard-iam-role",
  "version": "1.1.3",
  "description": "Dashboard IAM ROLE",
  "main": "lib/main.js",
  "typings": "lib/main.d.ts",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pyriter/dashboardIamRole.git"
  },
  "author": "Phong Vuong",
  "license": "GPL",
  "private": false,
  "bugs": {
    "url": "https://github.com/pyriter/dashboardIamRole/issues"
  },
  "homepage": "https://github.com/pyriter/dashboardIamRole#readme",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "clean": "rm -rf lib/",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "clearMocks": true,
    "resetMocks": true,
    "resetModules": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "html"
    ],
    "testTimeout": 300000,
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "aws-cdk-lib": "^2.183.0",
    "constructs": "^10.4.2"
  }
}
